@model MVCDemo.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}

<h2><b>Employee List</b></h2>
<table style="font-family:'Comic Sans MS'">
    <tr>
        <td><b>Employee Id :</b></td>
        <td>@Model.EmployeeId</td>
    </tr>
    <tr>
        <td><b>Employee Name :</b></td>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <td><b>Gender:</b></td>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <td><b>City :</b></td>
        <td>@Model.City</td>
    </tr>
</table>

<div>@Html.ActionLink("Go Back to Employee List", "Index",new {departmentId=Model.DepartmentId})</div>

